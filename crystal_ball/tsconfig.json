{
  "compilerOptions": {
    "target":"ES2015",
    "module": "ES2015",
    "allowJs": false,
    "sourceMap": true,
    "removeComments": true,
    "alwaysStrict": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "experimentalDecorators": true,
    "types": ["excalibur"],
  },
  "bazelOptions": {
    "workspaceName": ""
  }
}
